



/**
 * The Employee entity.
 */
entity Employee {
	/**
	* The firstname attribute.
	*/
	firstName String,
	lastName String,
	email String,
	phoneNumber String,
	hireDate Instant,
	salary Long,
	commissionPct Long
}






// Set service options to all except few
service all with serviceImpl
// Set an angular suffix
angularSuffix * with mySuffix
